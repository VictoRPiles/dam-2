<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="org.example.entity.FabricantesEntity" table="fabricantes" schema="examen2hiber">
        <id name="cif">
            <column name="CIF" sql-type="varchar(10)" length="10"/>
        </id>
        <property name="nombre">
            <column name="nombre" sql-type="varchar(30)" length="30" not-null="true"/>
        </property>
        <property name="descrip">
            <column name="descrip" sql-type="varchar(50)" length="50" not-null="true"/>
        </property>
        <property name="domicilio">
            <column name="domicilio" sql-type="varchar(30)" length="30" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>